{
  "name": "eat-well",
  "version": "1.0.0",
  "description": "Eat Well Restaurant",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node --inspect=5858 -r /usr/local/lib/node_modules/ts-node/register src/app.ts",
    "start:watch": "nodemon",
    "build": "tsc"
  },
  "nodemonConfig": {
    "ignore": [
      "*.test.ts",
      "*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "."
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "keywords": [
    "Express",
    "Node",
    "Restaurant"
  ],
  "author": "dipoengoro",
  "license": "MIT",
  "dependencies": {
    "ejs": "^3.1.8",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.10",
    "eslint": "^8.28.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.20"
  }
}
